#!/usr/bin/env Rscript

source("scripts/utils.R")

renv::restore()

packages <-
    c("renv"
      , "bookdown"
      , "caret"
      , "devtools"
      , "dplyr"
      , "effects"
      , "ggplot2"
      , "gtsummary"
      , "magrittr"
    )


install("plyr")

cat("Installing packages...\n")
ignore <- plyr::llply(packages, install, .progress = "text")
